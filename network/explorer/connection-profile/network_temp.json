{
  "name": "fabric-network",
  "version": "1.0.0",
  "client": {
    "tlsEnable": true,
    "adminCredential": {
      "id": "admin",
      "password": "123456"
    },
    "enableAuthentication": true,
    "organization": "TeslaMSP",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "channels": {
    "carchannel": {
      "peers": {
        "peer0.tesla.com": {}
      }
    }
  },
  "organizations": {
    "TeslaMSP": {
      "mspid": "TeslaMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/tesla.com/users/Admin@tesla.com/msp/keystore/priv_sk"
      },
      "peers": [
        "peer0.tesla.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/tesla.com/users/Admin@tesla.com/msp/signcerts/Admin@tesla.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.tesla.com": {
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/tesla.com/peers/peer0.tesla.com/tls/ca.crt"
      },
      "url": "grpc://peer0.tesla.com:7051"
    }
  }
